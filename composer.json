{
    "name": "gentenox/sentry-monolog-handler",
    "description": "Custom Sentry handler for Monolog for sending log context data to sentry events\nand it's focussed on ease-of-use and elegant syntax.",
    "keywords": ["symfony", "sentry"],
    "authors": [
        {
            "name": "Gentenox team",
            "email": "info@gentenox.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "SentryMonologHandler\\": "src/"
        }
    },
    "require": {
        "php": ">=8.1",
        "symfony/monolog-bundle": ">=3.8",
        "sentry/sentry-symfony": ">=4.2"
    },
    "minimum-stability": "stable",
    "license": "MIT"
}
